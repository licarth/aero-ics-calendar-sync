{
  "name": "aerogest-calendar",
  "version": "1.0.0",
  "description": "CalDAV server for aerogest flights",
  "main": "build/index.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "deploy": "npm run build && npm run gcloud-deploy",
    "dev": "nodemon",
    "gcloud-deploy": "gcloud functions deploy aero-ics-calendar-sync-staging --runtime nodejs12 --trigger-http --allow-unauthenticated --region us-central1 --entry-point flights",
    "local-deploy": "functions-emulator deploy aero-ics-calendar-sync-staging --trigger-http --allow-unauthenticated --region us-central1 --entry-point flights"
  },
  "author": "Thomas Carli <thomas.carli@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@google-cloud/functions-framework": "^1.7.1",
    "@types/axios": "^0.14.0",
    "@types/date-fns": "^2.6.0",
    "@types/escape-html": "^1.0.0",
    "@types/express": "^4.17.9",
    "@types/form-data": "^2.5.0",
    "@types/request": "^2.48.5",
    "@types/request-promise": "^4.1.47",
    "@types/set-cookie-parser": "^2.4.0",
    "axios": "^0.21.1",
    "date-fns": "^2.16.1",
    "date-fns-tz": "^1.0.12",
    "escape-html": "^1.0.3",
    "form-data": "^3.0.0",
    "jssoup": "0.0.12",
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "set-cookie-parser": "^2.4.6"
  },
  "devDependencies": {
    "express": "^4.17.1",
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
}
